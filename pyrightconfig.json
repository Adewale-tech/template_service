{
  "include": ["app", "services", "tests", "scripts"],
  "exclude": [".venv", "venv", ".git", "__pycache__", ".pytest_cache", ".mypy_cache", "dist", "build"],
  "ignore": ["**/migrations/**"],
  "defineConstant": {
    "DEBUG": true
  },
  "typeCheckingMode": "basic",
  "pythonVersion": "3.9",
  "pythonPlatform": "Linux",
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "reportOptionalMemberAccess": true,
  "reportOptionalSubscript": true,
  "reportOptionalCall": true,
  "reportOptionalIterable": true,
  "reportOptionalContextManager": true,
  "reportOptionalOperand": true,
  "reportOptionalRuntimeTypeCheckedCall": true,
  "reportConstantRedefinition": true,
  "reportInvalidTypeForm": true,
  "reportUnnecessaryIsInstance": false,
  "reportUnnecessaryCast": true,
  "reportUnnecessaryComparison": true,
  "reportAssignmentType": true,
  "reportPrivateImportUsage": true,
  "reportPrivateUsage": false,
  "reportInconsistentConstructor": true,
  "reportInconsistentMethodOverride": true,
  "reportIncompatibleMethodOverride": true,
  "reportIncompatibleVariableOverride": true,
  "reportInconsistentVariableDeclaration": true,
  "reportIncompatibleTypeChecker": false,
  "reportGeneralTypeIssues": true,
  "reportPropertyTypeMismatch": true,
  "reportFunctionMemberAccess": true,
  "reportUnusedImport": true,
  "reportUnusedClass": true,
  "reportUnusedFunction": true,
  "reportUnusedVariable": false,
  "reportDuplicateImport": true,
  "reportWildcardImportFromLibrary": true,
  "reportAbtractUsage": true,
  "reportArgumentType": true,
  "reportNoOverloadFound": true,
  "reportOperatorIssue": true,
  "reportReturnType": true,
  "reportReturnTypeVariable": true,
  "reportTypedDictNotRequiredAccess": true,
  "reportUnboundVariable": true,
  "reportUndefinedVariable": true,
  "reportMatchNotExhaustive": false,
  "reportSelfClsParameterName": true,
  "reportUnusedCallResult": false,
  "reportUnnecessaryLambda": true,
  "reportCallInDefaultArgumentValue": true,
  "reportUnusedExpression": false,
  "reportUnnecessaryStringEscapeSequence": true,
  "reportUnnecessaryStringConcatenation": true,
  "reportMultilineStringStartsOnNewLine": false,
  "reportImplicitStringConcatenation": false
}
